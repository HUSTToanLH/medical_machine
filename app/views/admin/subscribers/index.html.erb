<div id="page-wrapper">
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header">Khách hàng</h1>
    </div>
  </div>
  <div class="row" id="subscriber-table">
    <div class="col-lg-12">
      <div class="panel-body">
        <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-subscriber">
          <thead>
            <tr>
              <th>Email</th>
              <th>Tên</th>
              <th>Số điện thoại</th>
              <th>Tiêu đề</th>
              <th>Nội dung</th>
            </tr>
          </thead>
          <tbody>
            <% @subscribers.each do |subscriber| %>
              <tr class="odd gradeX">
                <td class="td-subscriber-email">
                  <p class="p-subscriber-email""><%= subscriber.email %></p>
                </td>
                <td class="td-subscriber-name">
                  <p class="p-subscriber-name""><%= subscriber.full_name %></p>
                </td>
                <td class="td-subscriber-phone">
                  <p class="p-subscriber-phone""><%= subscriber.phone %></p>
                </td>
                <td class="td-subscriber-title">
                  <p class="p-subscriber-title""><%= subscriber.title %></p>
                </td>
                <td class="td-subscriber-content">
                  <p class="p-subscriber-content""><%= subscriber.content %></p>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
