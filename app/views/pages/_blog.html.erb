                    <h5 class="tm-posts_item_title"><a href="#" title="<%= blog.title %>" rel="bookmark"><%= blog.title %></a></h5>
                    <div class="tm-posts_item_meta entry-meta">
                      <a href="#" class="post__date">
                      <time datetime="<%= blog.created_at %>" title="<%= blog.created_at %>"><%= blog.created_at.strftime(Settings.format.created_at) %></time>
                      </a><span class="post__comments"><a href="#" class="post__comments-link"><span class="linearicon linearicon-bubble"></span> <span class="text">0</span></a>
                      </span>
                    </div>
                  </div>
                  <div class="posts_item_content_footer">
                  </div>
                </div>
              </div>
            <% end %>
          </div>
        </div>
      </div>
